{
  "name": "drawspell-server",
  "main": "src/server.ts",
  "compatibility_date": "2025-12-17",
  "durable_objects": {
    "bindings": [
      {
        "name": "rooms",
        "class_name": "Room",
      },
    ],
  },
  "migrations": [
    {
      "tag": "v1",
      "new_sqlite_classes": ["Room"],
    },
  ],
  "observability": {
    "enabled": false,
    "head_sampling_rate": 1,
    "logs": {
      "enabled": true,
      "head_sampling_rate": 1,
      "invocation_logs": true,
    },
  },
  "env": {
    "development": {
      "vars": {
        "NODE_ENV": "development",
        "DRAWSPELL_PUBLIC_ORIGIN": "http://localhost:5173",
      },
    },
    "production": {
      "vars": {
        "NODE_ENV": "production",
        "DRAWSPELL_PUBLIC_ORIGIN": "https://drawspell.space",
        "POSTHOG_API_KEY": "phc_oYFcMPG9V4ARE4INIzfQQnLmADFN2GRLaYfDFiLSaQ6",
        "POSTHOG_API_HOST": "https://us.i.posthog.com",
      },
    },
  },
}
