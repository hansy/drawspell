{
  "$schema": "../../node_modules/wrangler/config-schema.json",
  "name": "mtg-signal",
  "main": "./index.ts",
  "compatibility_date": "2025-12-03",
  "durable_objects": {
    "bindings": [
      {
        "name": "WEBSOCKET_SERVER",
        "class_name": "SignalRoom"
      },
      {
        "name": "WEBRTC_SIGNALING",
        "class_name": "WebRTCSignaling"
      }
    ]
  },
  "migrations": [
    {
      "tag": "v1",
      "new_sqlite_classes": ["SignalRoom"]
    },
    {
      "tag": "v2",
      "new_classes": ["WebRTCSignaling"]
    }
  ],
  "observability": {
    "enabled": true
  }
}
